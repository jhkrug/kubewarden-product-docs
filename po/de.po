"metadata:\n"
"  name: reserved-instance-for-tenant-a\n"
"spec:\n"
"  image: ghcr.io/kubewarden/policy-server:v1.3.0\n"
"  replicas: 2\n"
"  serviceAccountName: ~\n"
"  env:\n"
"    - name: KUBEWARDEN_LOG_LEVEL\n"
"      value: debug\n"
msgstr ""
"apiVersion: policies.kubewarden.io/v1\n"
"kind: PolicyServer\n"
"metadata:\n"
"  name: reserved-instance-for-tenant-a\n"
"spec:\n"
"  image: ghcr.io/kubewarden/policy-server:v1.3.0\n"
"  replicas: 2\n"
"  serviceAccountName: ~\n"
"  env:\n"
"    - name: KUBEWARDEN_LOG_LEVEL\n"
"      value: debug\n"

#. type: delimited block =
#: docs/version-1.30/modules/en/pages/quick-start.adoc:202
